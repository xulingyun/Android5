<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="colorpicker">
        <attr name="primary_color" format="boolean"/>
        <attr name="accent_color" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="LoadingView">
        <attr name="circl_default_color" format="color"/>
        <attr name="circl_fill_color" format="color"/>
        <attr name="text_default_color" format="color"/>
        <attr name="text_fill_color" format="color"/>
        <attr name="progress" format="integer"/>
        <attr name="max_progress" format="integer"/>
    </declare-styleable>
</resources>